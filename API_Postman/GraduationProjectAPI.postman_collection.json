	"item": [
		{
			"name": "Verify Login with valid details",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Body matches string\", function () {\r",
							"    pm.expect(pm.response.text()).to.include(\"User exists!\");\r",
							"});\r",
							"pm.test(\"Response body responseCode matches HTTP status code\", function () {\r",
							"    // Get the actual HTTP status code\r",
							"    const httpStatus = pm.response.code;\r",
							"\r",
							"    // Try to parse the response body and extract responseCode if present\r",
							"    let body = {};\r",
							"    try {\r",
							"        body = pm.response.json();\r",
							"    } catch (e) {\r",
							"        // If not JSON, keep body as empty object\r",
							"    }\r",
							"\r",
							"    // Some APIs put the code under responseCode or status or code\r",
							"    const bodyStatus = body.responseCode ?? body.status ?? body.code;\r",
							"\r",
							"    // Ensure we found a numeric code in the body\r",
							"    pm.expect(bodyStatus, \"Response body must include a numeric status code field\").to.be.a(\"number\");\r",
							"\r",
							"    // Compare them\r",
							"    pm.expect(bodyStatus, `Body status (${bodyStatus}) should equal HTTP status (${httpStatus})`).to.equal(httpStatus);\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							""
						],
						"type": "text/javascript",
